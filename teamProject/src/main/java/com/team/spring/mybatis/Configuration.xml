<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<typeAliases>
		<typeAlias type="com.team.spring.ask.dto.AskDto" alias="askDto"/>
		<typeAlias type="com.team.spring.ask.dto.AskCommentDto" 
			alias="askCommentDto"/>
		<typeAlias type="com.team.spring.user.dto.UserDto" alias="userDto"/>
		<typeAlias type="com.team.spring.user.dto.UserCommentDto" 
			alias="userCommentDto"/>
	</typeAliases>
	
	<mappers>
		<mapper resource="com/team/spring/mybatis/AskMapper.xml" />
		<mapper resource="com/team/spring/mybatis/AskCommentMapper.xml" />
		<mapper resource="com/team/spring/mybatis/UserMapper.xml"/>
		<mapper resource="com/team/spring/mybatis/UserCommentMapper.xml"/>
	</mappers>	
</configuration>











